.swiper-cell {
  width: 100%;
  height: 144rpx;
  position: relative;
  background-color: red;
}
.swiper-cell .cell-item {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 3;
  background-color: white;
}
.swiper-cell .action {
  background-color: red;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100px;
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 2;
}
